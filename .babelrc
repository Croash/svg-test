{
  "presets": [ "es2015", "stage-0", "react" ],
  "plugins": [
    "transform-react-jsx",
    "add-module-exports",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    [
      "transform-imports",
      {
        "react-bootstrap": {
          "transform": "react-bootstrap/lib/${member}",
          "preventFullImport": true
        }
      }
    ]
  ]
}
